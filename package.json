{
  "name": "@trigidigital/n8n-nodes-chatbot-enchanced",
  "version": "1.0.2",
  "description": "Advanced chatbot functionality with smart rate limiting, message buffering, session management, and Redis integration for scalable conversation automation",
  "keywords": [
    "n8n-community-node-package",
    "chatbot",
    "ai",
    "redis",
    "valkey",
    "rate-limiting",
    "spam-detection",
    "message-buffer",
    "conversation",
    "automation",
    "messaging",
    "cache",
    "context-management",
    "user-interaction",
    "scalability",
    "session-management"
  ],
  "license": "MIT",
  "homepage": "https://trigidigital.com",
  "author": {
    "name": "TrigiDigital",
    "email": "admin@trigidigital.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trigi-Digital/n8n-nodes-chatbot-enchanced.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json",
    "test": "jest",
    "bmad:refresh": "bmad-method install -f -i codex",
    "bmad:list": "bmad-method list:agents",
    "bmad:validate": "bmad-method validate"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/ChatBotEnhanced/ChatBotEnhanced.node.js",
      "dist/nodes/ChatBotEnhanced/ChatBotEnhancedRedisTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^29.0.0",
    "n8n-workflow": "*",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "redis": "^5.6.1"
  }
}
